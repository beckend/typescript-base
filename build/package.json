{
    "name": "@linkening/typescript-base",
    "version": "0.0.1",
    "description": "base typescript tools",
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "build": "npm run patch:files && tsc --build ./tsconfig.build.json",
        "ci": "npm run prepare-release",
        "commit": "git-cz",
        "lint": "eslint './**/*.js' './**/*.ts' './**/*.tsx' --fix",
        "patch:files": "ts-node src/install/patchFiles.ts",
        "postinstall": "node ./build/src/install/installScript.js",
        "prepare-release": "concurrently 'npm run test:coverage' 'npm run lint' 'npm run build'",
        "release": "standard-version",
        "jest:integration": "jest -c jest.config.integration.js",
        "test:coverage": "jest --coverage",
        "test:watch-coverage": "jest --coverage --watch",
        "test:watch": "jest --watchAll",
        "test": "jest"
    },
    "main": "./build/src/index.js",
    "keywords": [],
    "author": "Linken Dinh",
    "license": "UNLICENSED",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@types/eslint": "^6.8.1",
        "@types/fs-extra": "^9.0.1",
        "@types/jest": "^25.2.3",
        "@types/lodash": "^4.14.152",
        "@types/stylelint": "^9.10.1",
        "@typescript-eslint/eslint-plugin": "^3.0.0",
        "@typescript-eslint/parser": "^3.0.0",
        "app-root-path": "^3.0.0",
        "concurrently": "^5.2.0",
        "cz-conventional-changelog": "^3.2.0",
        "eslint": "^7.0.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "fs-extra": "^9.0.0",
        "git-cz": "^4.5.0",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "jest-config": "^26.0.1",
        "jest-mock-console": "^1.0.0",
        "just-task": "^0.16.0",
        "lodash": "^4.17.15",
        "mock-fs": "^4.12.0",
        "prettier": "^2.0.5",
        "promisepipe": "^3.0.0",
        "standard-version": "^8.0.0",
        "ts-jest": "^26.0.0",
        "tslib": "^2.0.0"
    },
    "devDependencies": {
        "@schemastore/package": "0.0.5",
        "@testing-library/jest-dom": "^5.8.0",
        "@testing-library/react": "^10.0.4",
        "@types/expect-puppeteer": "^4.4.1",
        "@types/jest-environment-puppeteer": "^4.3.0",
        "@types/jest-image-snapshot": "^3.1.0",
        "@types/node": "^14.0.4",
        "@types/puppeteer": "^3.0.0",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/undertaker": "^1.2.3",
        "@types/yargs": "^15.0.5",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.2",
        "jest-environment-jsdom": "^26.0.1",
        "jest-environment-jsdom-global": "^2.0.2",
        "jest-image-snapshot": "^4.0.0",
        "jest-puppeteer": "^4.4.0",
        "puppeteer": "^3.1.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "stylelint": "^13.5.0",
        "stylelint-config-recommended": "^3.0.0",
        "ts-node": "^8.10.1",
        "typescript": "^3.9.3"
    },
    "peerDependencies": {
        "@testing-library/jest-dom": ">=5",
        "@testing-library/react": ">=10",
        "@types/expect-puppeteer": ">=4",
        "@types/jest-environment-puppeteer": ">=4",
        "@types/jest-image-snapshot": ">=3",
        "@types/puppeteer": ">=2",
        "@types/react": ">=16",
        "@types/react-dom": ">=16",
        "eslint-config-airbnb": ">=18",
        "eslint-plugin-react": ">=7",
        "eslint-plugin-react-hooks": ">=4",
        "jest-environment-jsdom": ">=26",
        "jest-environment-jsdom-global": ">=2",
        "jest-image-snapshot": ">=4",
        "jest-puppeteer": ">=4",
        "puppeteer": ">=3",
        "react": ">=16",
        "react-dom": ">=16",
        "stylelint": ">=13",
        "stylelint-config-recommended": ">=3",
        "typescript": ">=3.9"
    }
}
