{
    "name": "@linkening/typescript-base",
    "version": "0.0.1",
    "description": "base typescript tools",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "build": "npm run patch:files && tsc --build ./tsconfig.build.json",
        "build-new": "swc is not able to compile this project",
        "ci": "npm run prepare-release",
        "lint": "eslint './**/*.js' './**/*.ts' './**/*.tsx' --fix",
        "patch:files": "ts-node src/install/patchFiles.ts",
        "postinstall": "test -f ./build/src/install/installScript.js && node ./build/src/install/installScript.js",
        "prepare-release": "concurrently 'npm run test:coverage' 'npm run lint' 'npm run build'",
        "jest:integration": "jest -c jest.config.integration.js",
        "test:coverage": "jest --coverage",
        "test:watch-coverage": "jest --coverage --watch",
        "test:watch": "jest --watchAll",
        "test": "jest"
    },
    "main": "./build/src/index.js",
    "keywords": [],
    "author": "Linken Dinh",
    "license": "UNLICENSED",
    "dependencies": {
        "@swc/helpers": "^0.2.12",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "app-root-path": "^3.0.0",
        "async": "^3.2.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.23.4",
        "fs-extra": "^10.0.0",
        "jest": "^27.0.4",
        "jest-config": "^27.0.4",
        "just-task": "^1.2.0",
        "lodash": "^4.17.21",
        "prettier": "^2.3.1",
        "promisepipe": "^3.0.0",
        "ts-jest": "^27.0.3",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@schemastore/package": "0.0.6",
        "@swc-node/jest": "^1.3.1",
        "@swc/cli": "^0.1.46",
        "@swc/core": "^1.2.61",
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "11.2.7",
        "@types/async": "^3.2.6",
        "@types/eslint": "^7.2.13",
        "@types/fs-extra": "^9.0.11",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.170",
        "@types/node": "^15.12.4",
        "@types/react": "17.0.11",
        "@types/react-dom": "17.0.8",
        "@types/stylelint": "^13.13.0",
        "@types/undertaker": "^1.2.6",
        "@types/yargs": "^17.0.0",
        "@typescript-eslint/parser": "^4.27.0",
        "concurrently": "^6.2.0",
        "eslint": "^7.29.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest-environment-jsdom": "^27.0.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "stylelint": "13.13.1",
        "stylelint-config-recommended": "5.0.0",
        "ts-node": "^10.0.0",
        "typescript": "4.3.4"
    },
    "peerDependencies": {
        "@swc-node/jest": "^1.3.1",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.8",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest-environment-jsdom": ">=27",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "stylelint": "^13.13.1",
        "stylelint-config-recommended": "^5.0.0",
        "typescript": "^4.3.4"
    }
}
