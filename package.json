{
  "name": "@linkening/typescript-base",
  "version": "0.0.1",
  "description": "base typescript tools",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "npm run patch:files && tsc --build ./tsconfig.build.json",
    "ci": "npm run prepare-release",
    "commit": "git-cz",
    "lint": "eslint . --fix",
    "patch:files": "ts-node src/install/patchFiles.ts",
    "postinstall": "node ./build/src/install/installScript.js",
    "prepare-release": "concurrently 'npm run test:coverage' 'npm run lint' 'npm run build'",
    "release": "standard-version",
    "jest:integration": "jest -c jest.config.integration.js",
    "test:coverage": "jest --coverage",
    "test:watch-coverage": "jest --coverage --watch",
    "test:watch": "jest --watchAll",
    "test": "jest"
  },
  "main": "./build/src/index.js",
  "keywords": [],
  "author": "Linken Dinh",
  "license": "UNLICENSED",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/eslint": "^6.1.7",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/stylelint": "^9.10.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "app-root-path": "^3.0.0",
    "concurrently": "^5.1.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra": "^8.1.0",
    "git-cz": "^4.2.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "jest-mock-console": "^1.0.0",
    "just-task": "^0.14.3",
    "lodash": "^4.17.15",
    "mock-fs": "^4.10.4",
    "npm-check-updates": "^4.0.1",
    "prettier": "^1.19.1",
    "promisepipe": "^3.0.0",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.1.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@schemastore/package": "0.0.5",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.3.3",
    "@types/expect-puppeteer": "^4.4.0",
    "@types/jest-environment-puppeteer": "^4.3.0",
    "@types/jest-image-snapshot": "^2.11.1",
    "@types/node": "^13.5.2",
    "@types/puppeteer": "^2.0.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/undertaker": "^1.2.2",
    "@types/yargs": "^15.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest-environment-jsdom": "^25.1.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-image-snapshot": "^2.12.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^2.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@testing-library/jest-dom": ">=5",
    "@testing-library/react": ">=9",
    "@types/expect-puppeteer": ">=4",
    "@types/jest-environment-puppeteer": ">=4",
    "@types/jest-image-snapshot": ">=2",
    "@types/puppeteer": ">=2",
    "@types/react": ">=16",
    "@types/react-dom": ">=16",
    "eslint-config-airbnb": ">=18",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=2",
    "jest-environment-jsdom": ">=25",
    "jest-environment-jsdom-global": ">=1",
    "jest-image-snapshot": ">=2",
    "jest-puppeteer": ">=4",
    "puppeteer": ">=2",
    "react": ">=16",
    "react-dom": ">=16",
    "stylelint": ">=13",
    "stylelint-config-recommended": ">=3",
    "typescript": ">=3.7"
  }
}
